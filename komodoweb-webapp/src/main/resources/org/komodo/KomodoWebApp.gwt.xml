<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.5/distro-source/core/src/gwt-module.dtd">
<module>
  <inherits name="org.uberfire.UberfireClientAll"/>
  <inherits name="org.jboss.errai.ui.UI"/>  
  <inherits name="org.jboss.errai.bus.ErraiBus"/>  
  <inherits name="org.jboss.errai.ioc.Container"/>  
  <inherits name="org.jboss.errai.enterprise.CDI"/> 

  <inherits name="org.jboss.errai.common.ErraiCommon" />
  <inherits name="org.jboss.errai.marshalling.ErraiMarshalling" />
  <inherits name="org.jboss.errai.databinding.DataBinding" />
  <inherits name="org.jboss.errai.enterprise.Jaxrs" />
  <inherits name="org.jboss.errai.ui.nav.Navigation"/>
  
  <inherits name="org.uberfire.security.UberfireSecurityClient"/>
  <inherits name="org.uberfire.client.views.PatternFly"/>

  <inherits name='com.github.gwtd3.D3' />
  <inherits name="org.komodo.KomodoSpi"/>
  
  <!-- SuperDevMode -->
  <add-linker name="xsiframe"/>
  <set-configuration-property name="devModeRedirectEnabled" value="true"/>
  <set-property name="compiler.useSourceMaps" value="true"/>
  <set-configuration-property name='xsiframe.failIfScriptTag' value='false'/>
  
  <set-property name="gwt.logging.popupHandler" value="DISABLED"/>  
  
  <source path='web/client' />
  <source path='web/share' />
</module>
