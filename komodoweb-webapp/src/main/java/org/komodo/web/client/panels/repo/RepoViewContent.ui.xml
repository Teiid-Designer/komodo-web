<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <ui:style>
      .noTopMarginStyle {
        margin-top:0px;
        margin-bottom:0px;
      }
    </ui:style>
    
	<b:PanelGroup b:id="accordion" addStyleNames="{style.noTopMarginStyle}">
		<b:Panel>
			<b:PanelHeader>
				<b:Heading size="H4">
				    <b:Icon type="TABLE" size="LARGE" />
					<b:Anchor dataToggle="COLLAPSE" dataParent="#accordion" dataTarget="#collapseView">Views</b:Anchor>
				</b:Heading>
			</b:PanelHeader>
			<b:PanelCollapse b:id="collapseView" ui:field="collapseView">
				<b:PanelBody>
				</b:PanelBody>
			</b:PanelCollapse>
		</b:Panel>
	</b:PanelGroup>
	
</ui:UiBinder>

